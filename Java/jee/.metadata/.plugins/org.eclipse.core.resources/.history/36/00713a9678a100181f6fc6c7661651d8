package mt.practice;

public class Lab762 {

}

class MyThread762 extends Thread{
	@Override
	public void run() {
		Thread th = Thread.currentThread();
		for (int i = 1; i <= 10; i++){
			System.out.println(th.getName() + " - value is :" + i + "\t" + th.getState());
			try{
				Thread.sleep(500);
			}catch (Exception e){
				e.printStackTrace();
			}
		}
		super.run();
	}
}