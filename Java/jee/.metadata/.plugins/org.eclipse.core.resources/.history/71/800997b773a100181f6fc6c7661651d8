package mt.practice;

public class Lab751 {
public static void main(String[] args) {
	
}
}

class MyThread extends Thread{
	public void run(){
		Thread th = Thread.currentThread();
		for (int i = 0; i <= 10; i++){
			System.out.println(th.getName() + "-value is :" + i);
			try{
				Thread.sleep(500);
			}catch (Exception e){
				e.printStackTrace();
			}
		}
	}
}